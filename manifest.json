{
    "manifest_version": 3,
    "name": "Poker Odds Calculator",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://www.pokernow.club/*"
            ],
            "js": [
                "dist/content.bundle.js"
            ]
        }
    ],
    "background": {
        "service_worker": "dist/background.bundle.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}
